<?xml version="1.0" encoding="utf-8"?>
<acrn-config>
  <hv>
    <BUILD_TYPE>debug</BUILD_TYPE>
    <FEATURES>
      <IVSHMEM>
        <IVSHMEM_REGION>
          <NAME>tee_ivishmem</NAME>
          <PROVIDED_BY>Hypervisor</PROVIDED_BY>
          <IVSHMEM_SIZE>4</IVSHMEM_SIZE>
          <IVSHMEM_VMS>
            <IVSHMEM_VM>
              <VM_NAME>VM0</VM_NAME>
              <VBDF>00:0c.0</VBDF>
            </IVSHMEM_VM>
            <IVSHMEM_VM>
              <VM_NAME>VM1</VM_NAME>
              <VBDF>00:0c.0</VBDF>
            </IVSHMEM_VM>
          </IVSHMEM_VMS>
        </IVSHMEM_REGION>
      </IVSHMEM>
      <RELOC_ENABLED>y</RELOC_ENABLED>
      <SCHEDULER>SCHED_BVT</SCHEDULER>
      <MULTIBOOT2_ENABLED>y</MULTIBOOT2_ENABLED>
      <SPLIT_LOCK_DETECTION_ENABLED>n</SPLIT_LOCK_DETECTION_ENABLED>
      <UC_LOCK_DETECTION_ENABLED>n</UC_LOCK_DETECTION_ENABLED>
      <SECURITY_VM_FIXUP>y</SECURITY_VM_FIXUP>
      <HYPERV_ENABLED>y</HYPERV_ENABLED>
      <ACPI_PARSE_ENABLED>y</ACPI_PARSE_ENABLED>
      <L1D_VMENTRY_ENABLED>n</L1D_VMENTRY_ENABLED>
      <MCE_ON_PSC_ENABLED>y</MCE_ON_PSC_ENABLED>
      <CPU_PERFORMANCE_POLICY>Performance</CPU_PERFORMANCE_POLICY>
      <RDT>
        <RDT_ENABLED>n</RDT_ENABLED>
        <CDP_ENABLED>n</CDP_ENABLED>
        <VCAT_ENABLED>n</VCAT_ENABLED>
      </RDT>
      <KEEP_IRQ_DISABLED>n</KEEP_IRQ_DISABLED>
      <IOMMU_ENFORCE_SNP>n</IOMMU_ENFORCE_SNP>
    </FEATURES>
    <DEBUG_OPTIONS>
      <SERIAL_CONSOLE>/dev/ttyS4</SERIAL_CONSOLE>
      <MEM_LOGLEVEL>5</MEM_LOGLEVEL>
      <NPK_LOGLEVEL>5</NPK_LOGLEVEL>
      <CONSOLE_LOGLEVEL>3</CONSOLE_LOGLEVEL>
      <CONSOLE_VM>1</CONSOLE_VM>
    </DEBUG_OPTIONS>
    <MEMORY>
      <STACK_SIZE>0x2000</STACK_SIZE>
    </MEMORY>
    <CAPACITIES>
      <MAX_PCI_DEV_NUM>96</MAX_PCI_DEV_NUM>
      <MAX_PT_IRQ_ENTRIES>256</MAX_PT_IRQ_ENTRIES>
      <MAX_MSIX_TABLE_NUM>64</MAX_MSIX_TABLE_NUM>
      <MAX_EMULATED_MMIO>16</MAX_EMULATED_MMIO>
      <MAX_VM_NUM>16</MAX_VM_NUM>
      <MAX_IOAPIC_NUM>1</MAX_IOAPIC_NUM>
      <MAX_PCI_BUS_NUM>0</MAX_PCI_BUS_NUM>
      <MAX_IOAPIC_LINES>120</MAX_IOAPIC_LINES>
    </CAPACITIES>
    <MISC_CFG>
      <GPU_SBDF>0x00000010</GPU_SBDF>
    </MISC_CFG>
  </hv>
  <vm id="0">
    <vm_type>STANDARD_VM</vm_type>
    <console_vuart>COM Port 1</console_vuart>
    <bvt_weight>9</bvt_weight>
    <memory>
       <hpa_region>
        <start_hpa>0x110000000</start_hpa>
        <size_hpa>128</size_hpa>
       </hpa_region>
    </memory>
    <cpu_affinity>
      <pcpu>
        <pcpu_id>0</pcpu_id>
        <real_time_vcpu>n</real_time_vcpu>
      </pcpu>
    </cpu_affinity>
    <os_config>
      <kern_type>KERNEL_ELF</kern_type>
      <kern_mod>tee_elf</kern_mod>
      <bootargs></bootargs>
    </os_config>
    <mmio_resources>
      <TPM2>y</TPM2>
      <p2sb>n</p2sb>
    </mmio_resources>
    <lapic_passthrough>n</lapic_passthrough>
    <io_completion_polling>n</io_completion_polling>
    <virtual_cat_support>n</virtual_cat_support>
    <virtual_cat_number>0</virtual_cat_number>
    <epc_section>
      <base>0</base>
      <size>0</size>
    </epc_section>
    <load_order>PRE_LAUNCHED_VM</load_order>
    <name>VM0</name>
    <own_pcpu>n</own_pcpu>
    <priority>PRIO_LOW</priority>
    <companion_vmid>65535</companion_vmid>
  </vm>
  <vm id="1">
    <vm_type>STANDARD_VM</vm_type>
    <console_vuart>COM Port 1</console_vuart>
    <os_config>
      <kern_type>KERNEL_ELF</kern_type>
      <kern_mod>kernelflinger</kern_mod>
      <bootargs>serail_baseaddr=0x3f8 serail_type=1 serail_regwidth=1 console=ttyS0,115200</bootargs>
    </os_config>
    <lapic_passthrough>n</lapic_passthrough>
    <nested_virtualization_support>n</nested_virtualization_support>
    <virtual_thermal_monitor>y</virtual_thermal_monitor>
    <load_order>SERVICE_VM</load_order>
    <name>VM1</name>
    <own_pcpu>n</own_pcpu>
    <priority>PRIO_LOW</priority>
    <companion_vmid>65535</companion_vmid>
  </vm>
  </acrn-config>
